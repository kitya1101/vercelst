{
	"version": 2,
	"builds": [
		{
			"src": "server/app.py",
			"use": "@vercel/python"
		},
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": ".svelte-kit/output/client"
			}
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "server/app.py"
		},
		{
			"src": "/(.*)",
			"dest": "/.svelte-kit/output/server/index.js"
		}
	]
}
